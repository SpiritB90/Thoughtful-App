<%- include('../partials/header') %>

<header>
    <nav id="logout-btn">
        <a id="logout-tag" href="/">Logout</a>
    </nav>
</header>

<img id="media-bubble" src="/images/ConversationBubble.png" alt="Conversation Bubble">
<h1 id="title3">ThoughtFUL...</h1>

<h2>MEDIA</h2>

<nav class="media-nav">
    <a href="/home">Home</a>
    <a href="/new">New Entry</a>
    <a href="/favorites">Favorites</a>
    <!-- <a href="/">Logout</a> -->
</nav>

<tbody>
    <% posts.forEach(function(p) { %>
      <tr>
        <td><%= p.links %></td>
            <form action="/media/<%= p._id %>?_method=PUT" method="POST" >
                <button type="submit">Update</button>
            </form>
            <form action="/media/<%= p._id %>?_method=DELETE" method="POST">
                <button type="submit">Delete Post</button>
            </form>
      </tr>
      <% }); %>
</tbody>

<%- include('../partials/footer') %>